{
  "name": "@loli-feature-flags/loli-ui",
  "version": "1.0.0-alpha.2",
  "keywords": [
    "featureflags",
    "flags",
    "splittesting",
    "variants",
    "abtesting",
    "gradualrollout",
    "loli",
    "ui",
    "userinterface"
  ],
  "description": "Loli Feature Flags UI",
  "repository": {
    "type": "git",
    "url": "https://github.com/Loli-Feature-Flags/loli-ui"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": {
    "name": "Peter Kuhmann",
    "email": "info@peter-kuhmann.de",
    "url": "https://github.com/peter-kuhmann"
  },
  "files": [
    "/dist",
    "LICENSE"
  ],
  "types": "dist/main.d.ts",
  "type": "module",
  "main": "./dist/loli-ui.umd.cjs",
  "module": "./dist/loli-ui.js",
  "exports": {
    ".": {
      "import": "./dist/loli-ui.js",
      "require": "./dist/loli-ui.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@internationalized/date": "^3.5.4",
    "@loli-feature-flags/loli-sdk": "1.0.0-alpha.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "pinia": "^2.1.7",
    "radix-vue": "^1.8.2",
    "vue": "^3.4.21",
    "vue-i18n": "9",
    "vuedraggable": "next",
    "zod": "^3.22.8"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@total-typescript/ts-reset": "^0.5.1",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.25",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.38",
    "postcss-scope": "^1.7.3",
    "prettier": "^3.0.3",
    "sass": "^1.72.0",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.0",
    "vite": "^5.1.5",
    "vite-plugin-dts": "^3.9.1",
    "vue-tsc": "^2.0.22"
  }
}
